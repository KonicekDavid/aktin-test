{
	"name": "konicekdavid/api-test",
	"description": "Mini REST API app",
	"keywords": ["nette"],
	"type": "project",
	"license": ["MIT", "BSD-3-Clause", "GPL-2.0", "GPL-3.0"],
	"require": {
		"php": ">= 8.2",
		"nette/application": "^3.2.3",
		"nette/bootstrap": "^3.2",
		"nette/caching": "^3.2",
		"nette/database": "^3.2",
		"nette/di": "^3.2",
		"nette/forms": "^3.2",
		"nette/http": "^3.3",
		"nette/mail": "^4.0",
		"nette/robot-loader": "^4.0",
		"nette/security": "^3.2",
		"nette/utils": "^4.0",
		"latte/latte": "^3.0",
		"tracy/tracy": "^2.10",

		"adhocore/jwt": "^1.1",
		"contributte/api-router": "^6.0",
		"nettrine/orm": "^0.10.0",
		"nettrine/dbal": "^0.10.2"
    },
	"require-dev": {
		"nette/tester": "^2.5",
		"symfony/thanks": "^1",
		"squizlabs/php_codesniffer": "^3.12",
		"phpstan/phpstan": "^2.1",
		"mockery/mockery": "^1.6"
	},
	"autoload": {
		"psr-4": {
			"App\\": "app",
			"Tests\\": "tests"
		}
	},
	"minimum-stability": "stable",
	"config": {
		"allow-plugins": {
			"symfony/thanks": true
		}
	}
}
